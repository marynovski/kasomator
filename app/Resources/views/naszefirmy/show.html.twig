{% extends 'base.html.twig' %}

{% block body %}
    <h1>Dane firmy</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ naszeFirmy.id }}</td>
            </tr>
            <tr>
                <th>Nazwa</th>
                <td>{{ naszeFirmy.nazwa }}</td>
            </tr>
            <tr>
                <th>Adres</th>
                <td>{{ naszeFirmy.adres }}</td>
            </tr>
            <tr>
                <th>Miasto</th>
                <td>{{ naszeFirmy.miasto }}</td>
            </tr>
            <tr>
                <th>Kodpocztowy</th>
                <td>{{ naszeFirmy.kodPocztowy }}</td>
            </tr>
            <tr>
                <th>Nip</th>
                <td>{{ naszeFirmy.nip }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('operacje_nieprzypisane') }}">Operacje</a>
        </li>
    </ul>
{% endblock %}
